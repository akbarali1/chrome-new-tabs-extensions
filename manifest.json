{
  "name": "Akbarali",
  "description": "Akbarali",
  "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "icon.png",
	"128": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  
  "version": "2",
  "minimum_chrome_version": "9",
  "manifest_version": 2
}
